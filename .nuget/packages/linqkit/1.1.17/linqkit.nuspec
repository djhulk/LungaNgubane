<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>LinqKit</id>
    <version>1.1.17</version>
    <authors>Joseph Albahari,Tomas Petricek,Scott Smith,Tuomas Hietanen,Stef Heyenrath</authors>
    <owners>Joseph Albahari,Tomas Petricek,Scott Smith,Tuomas Hietanen,Stef Heyenrath</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <licenseUrl>https://licenses.nuget.org/MIT</licenseUrl>
    <projectUrl>https://github.com/scottksmith95/LINQKit</projectUrl>
    <description>LinqKit.EntityFramework contains extensions for LINQ to SQL and Entity Framework. Include(...) and IAsync are supported.</description>
    <releaseNotes>See ReleaseNotes.md</releaseNotes>
    <tags>linq EF EntityFramework Entity Framework</tags>
    <repository type="git" url="https://github.com/scottksmith95/LINQKit" />
    <dependencies>
      <group targetFramework=".NETFramework4.5">
        <dependency id="EntityFramework" version="6.2.0" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETStandard2.1">
        <dependency id="EntityFramework" version="6.3.0" exclude="Build,Analyzers" />
      </group>
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="Microsoft.CSharp" targetFramework=".NETFramework4.5" />
      <frameworkAssembly assemblyName="System" targetFramework=".NETFramework4.5" />
    </frameworkAssemblies>
  </metadata>
</package>